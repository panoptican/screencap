{
	"compilerOptions": {
		"composite": true,
		"target": "ES2020",
		"lib": ["ES2020"],
		"module": "ESNext",
		"moduleResolution": "bundler",
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true,
		"strict": true,
		"skipLibCheck": true,
		"noEmit": true,
		"types": ["node"],
		"resolveJsonModule": true,
		"isolatedModules": true
	},
	"include": ["electron.vite.config.ts", "electron/**/*.ts"]
}
